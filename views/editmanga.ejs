<form action="/manga/edit/<%= manga.id %>" method="POST">
    <input type="text" name="title" placeholder="title" value=<%= manga.title%>>
    <input type="text" name="author" placeholder="author" value="<%= manga.author%>">
    <input type="number" name="rating" placeholder="rating" value="<%= manga.rating%>">
    <input type="text" name="img" placeholder="img" value="<%= manga.img%>">
    <input type="number" name="pages" placeholder="pages" value="<%= manga.pages%>">
    
    <button type="submit">Edit</button>
</form>

